{
  "presets": [
    "@babel/env",
    "@babel/preset-stage-3",
    "@babel/typescript"
  ],
  "plugins": [
    ["module-resolver", {
      "cwd": "babelrc",
      "root": ["./src"],
      "extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx",
        ".json"
      ]
    }]
  ],
  "comments": false
}
